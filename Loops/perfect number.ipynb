   "source": [
    "def is_perfect(n):\n",
    "    divisors_sum = sum(i for i in range(1, n) if n % i == 0)\n",
    "    return divisors_sum == n\n",
    "\n",
    "def find_perfect_numbers(limit):\n",
    "    perfect_numbers = [n for n in range(1, limit + 1) if is_perfect(n)]\n",
    "    return perfect_numbers"
   ]
  }
